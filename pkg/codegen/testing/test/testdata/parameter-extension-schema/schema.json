{
  "version": "1.2.3",
  "name": "paramExample",
  "extension": {
    "name": "example",
    "version": "1.2.3"
  },
  "parameter": {
    "hello": "world",
    "data": [1, 2, 3]
  },
  "allowedPackageNames": ["foo", "bar"],
  "pluginDownloadURL": "example.com/download",
  "resources": {
    "paramExample:index:Resource": {
      "properties": {
        "bar": {
          "type": "string",
          "secret": true
        },
        "baz": {
          "type": "string",
          "secret": true
        }
      },
      "inputProperties": {
        "bar": {
          "type": "string",
          "secret": true,
          "willReplaceOnChanges": true
        }
      },
      "type": "object"
    }
  }
}
